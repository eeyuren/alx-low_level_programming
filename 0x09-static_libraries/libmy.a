#include "main.h"
#include <ctype.h>

/**
 * _isupper - Checks for an uppercase letter
 * @c: The int value of character to evaluate
 *
 * Return: 1 if uppercase, other case 0.
 */
int _isupper(int c)
{
	if (c >= 65 && c <= 90)
		return (1);
	return (0);
}

